{
  "name": "AioriaGetmo",
  "version": "0.1.0",
  "summary": "Biblioteca de integração com o Framework de Push da Getmo",
  "description": "Integração com o Framework de Push da Getmo. Desenvolvido pela Aioria através do código disponível pela própria Getmo.",
  "homepage": "https://github.com/aioriash/AioriaGetmo.git",
  "license": "MIT",
  "authors": {
    "Gustavo Tagliari": "gustavo@aioria.com.br"
  },
  "source": {
    "git": "https://github.com/aioriash/AioriaGetmo.git",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/**/*",
  "resource_bundles": {
    "AioriaGetmo": [
      "Pod/Assets/*.png"
    ]
  },
  "frameworks": [
    "CoreLocation",
    "AdSupport"
  ],
  "preserve_paths": "Assets/SmartpushSDK.framework",
  "xcconfig": {
    "OTHER_LDFLAGS": "-framework SmartpushSDK"
  },
  "vendored_frameworks": "Assets/SmartpushSDK.framework"
}
